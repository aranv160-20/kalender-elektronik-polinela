<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- State ketika elemen dipilih (misalnya, memiliki event) -->
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <!-- Warna abu-abu untuk penanda tanggal dengan event -->
            <solid android:color="#CCCCCC" />
        </shape>
    </item>

    <!-- State default (tanpa event) -->
    <item>
        <!-- Biarkan kosong jika tidak ada perubahan warna -->
    </item>

</selector>
